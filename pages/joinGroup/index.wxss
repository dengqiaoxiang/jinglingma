	height: 26rpx;
  margin-right: 4rpx;
}

.del-intro .title{
  margin: 0 20rpx;
  font-size: 34rpx;
	color: #61d96b;
}


.del-img{
  width: 100%;
  margin: 50rpx 0;
}

.del-bot{
  width: 100%;
	height: 98rpx;
  background-color: #fff;
}
.del-bot>view{height: 100%;}
