	border-radius: 10rpx;
}

.peoples{
  padding: 30rpx 30rpx 40rpx;
}
.peoples>view{
  position: relative;
  margin-right: 30rpx;
}
.peoples>view:last-child{  margin-right: 0;}

.peoples image{
  width: 110rpx;
	height: 110rpx;
	background-color: #f6f8fb;
  border-radius: 50%;
}
.peoples>view>view{
  position: absolute;
  bottom: -10rpx;
  width: 72rpx;
	height: 30rpx;
  margin-left: 20rpx;
  font-size: 22rpx;
	color: #fff;
  text-align: center;
	background-color: #f03a58;
	border-radius: 20rpx;
}
.group-time{
  font-size: 24rpx;
	color: #444;
}

.groupRule{
  padding: 23rpx 30rpx;
  font-size: 24rpx;
	color: #000;
}

.groupRule>view{
  margin-right: 30rpx;
  color: #666;
}


.serve-item>view{
  font-size: 26rpx;
	color: #666;
}
